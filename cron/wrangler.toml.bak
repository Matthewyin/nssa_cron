name = "nssa-cron"
main = "worker.js"
compatibility_date = "2025-08-08"

# 环境变量配置
[vars]
FIREBASE_API_KEY = "aaa"
FIREBASE_AUTH_DOMAIN = "project.firebaseapp.com" # 需要替换为实际的域名
FIREBASE_PROJECT_ID = "nssa-cron" # 需要替换为实际的项目ID
FIREBASE_STORAGE_BUCKET = "nssa-cron.appspot.com"
FIREBASE_MESSAGING_SENDER_ID = "XXXXXXXXXXXX" # 需要替换为实际的发送者ID
FIREBASE_APP_ID = "1:XXXXXXXXXXXX:web:XXXXXXXXXXXX" # 需要替换为实际的应用ID

[[rules]]
type = "ESModule"
globs = ["worker.js"]